server:
    port: 8762
spring:
  application:
    name: API-GATEWAY
  cloud:
    gateway:
      discovery:
        locator:
          enabled: true
          lower-case-service-id: true
      routes:
        - id: QUERIES-SERVICE
          uri: lb://QUERIES-SERVICE
          predicates:
            - Path=/queries/**
            - Path=/queryLanguageType/**
				- id: CODER-SERVICE
				  uri: lb://CODER-SERVICE
					predicates:
						- Path=/coder/**	
eureka:
  client:
    serviceUrl:
      defaultZone: http://localhost:8761/eureka/
    registerWithEureka: true
    fetchRegistry: true
