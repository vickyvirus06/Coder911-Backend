package com.coder911.coder.mapping.data;

import com.coder911.coder.data.CoderData;
import com.coder911.coder.entity.User;

public class Mapping {

	public static User getEntity(CoderData coderData) {
		
		if (coderData != null) {
			User user = new User();
			user.setUserName(coderData.getUserName());
			user.setEmail(coderData.getEmail());
			user.setPassword(coderData.getPassword());
			return user;
		}
		return null;
	}

}
