package com.coder911.user.mapping.data;

import com.coder911.user.data.UserData;
import com.coder911.user.dto.UserDTO;

public class Mapping {

	public static UserData getObject(UserDTO userDTO) {
		if (userDTO != null) {
			UserData userData = new UserData();
			userData.setUserName(userDTO.getUserName());
			userData.setEmail(userDTO.getEmail());
			userData.setPassword(userDTO.getPassword());
			return userData;
		}
		
		return null;
	}

}
